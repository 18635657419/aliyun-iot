{
    "name": "simps-cloud/aliyun-iot",
    "type": "project",
    "description": "calling AliYun IoT services using the Simps framework.",
    "keywords": [
        "php",
        "swoole",
        "coroutine",
        "simple",
        "simps",
        "aliyun",
        "iot"
    ],
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.1",
        "ext-swoole": ">=4.4",
        "simple-swoole/simps": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": ["app/functions.php"]
    },
    "bin": [
        "bin/simps.php"
    ],
    "config": {
        "optimize-autoloader": true
    }
}
